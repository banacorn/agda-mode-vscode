{
  "name": "agda-mode-vscode",
  "jsx": { "version": 4, "mode": "classic" },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "@glennsl/rescript-json-combinators",
    "rescript-vscode",
    "rescript-webapi",
    "rescript-nodejs",
    "@rescript/react",
    "@rescript/core"
  ],
  "bs-dev-dependencies": [
    "rescript-mocha"
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-bs-g"
  ],
  "ppx-flags": []
}